var dir = point_direction(other.x, other.y, x, y);
hspeed_push_ += lengthdir_x(.1, dir);
vspeed_push_ += lengthdir_y(.1, dir);
if place_meeting(x+hspeed_push_, y, OWall) {
		while !place_meeting(x+sign(hspeed_push_), y, OWall) {
			x += sign(hspeed_push_);
		}
		hspeed_push_ = 0;
	}
	x += hspeed_push_;

	if place_meeting(x, y+vspeed_push_, OWall) {
		while !place_meeting(x, y+sign(vspeed_push_), OWall) {
			y += sign(vspeed_push_);
		}
		vspeed_push_ = 0;
	}
	y += vspeed_push_;
